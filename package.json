    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "jsonwebtoken": "^8.5.1",
    "mailer": "^0.6.7",
    "moment": "^2.24.0",
    "mongoose": "^5.4.20",
    "multer": "^1.4.2",
    "node": "^13.7.0",
    "nodemailer": "^6.4.16",
    "react-redux": "^5.0.7",
    "socket.io": "^2.2.0"
  },
